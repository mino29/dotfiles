{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#8fbcbb",
          "style": "powerline",
          "template": "({{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Major }}.{{ .Minor }}{{ end }})",
          "type": "python"
        },
        {
          "foreground": "#8fbcbb",
          "style": "plain",
          "template": " ➜",
          "type": "text"
        },
        {
          "foreground": "#8fbcbb",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#FF8A80",
          "properties": {
            "branch_icon": " "
          },
          "template": "<#56B6C2>(</>{{ .HEAD }}<#56B6C2>)</>"
        },
        {
          "foreground": "#BF616A",
          "style": "plain",
          "template": " ✗",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
