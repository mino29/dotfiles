{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#eceff4",
          "foreground": "#3B4252",
          "leading_diamond": "╭─",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#a3be8c",
          "foreground": "#3B4252",
          "powerline_symbol": "",
          "style": "powerline",
          "template": "  ",
          "type": "root"
        },
        {
          "background": "#81a1c1",
          "foreground": "#3B4252",
          "powerline_symbol": "",
          "properties": {
            "home_icon": " ~",
            "max_depth": 2,
            "style": "letter"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#A3BE8C",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ebcb8b{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#d08770{{ end }}",
            "{{ if gt .Ahead 0 }}#88c0d0{{ end }}",
            "{{ if gt .Behind 0 }}#bf616a{{ end }}"
          ],
          "foreground": "#3B4252",
          "powerline_symbol": "",
          "properties": {
            "branch_icon": " ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": "   {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#3B4252",
          "background_templates": [
            "{{ if gt .Code 0 }}#bf616a{{ end }}"
          ],
          "foreground": "#ebcb8b",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ if gt .Code 0 }}{{ .Meaning }}{{ else }}✔{{ end }} ",
          "type": "exit"
        },
        {
          "background": "#ebcb8b",
          "foreground": "#3B4252",
          "invert_powerline": true,
          "powerline_symbol": "",
          "style": "powerline",
          "template": " {{ .FormattedMs }}  ",
          "type": "executiontime"
        },
        {
          "background": "#689f63",
          "foreground": "#eceff4",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#00acd7",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "#4063D8",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "julia"
        },
        {
          "background": "#81a1c1",
          "foreground": "#3b4252",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#4063D8",
          "foreground": "#ec2729",
          "style": "plain",
          "template": "<#eceff4>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "java"
        },
        {
          "background": "#4063D8",
          "foreground": "#0d6da8",
          "style": "plain",
          "template": "<#eceff4>(</>{{ if .Unsupported }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "dotnet"
        },
        {
          "background": "#3B4252",
          "foreground": "#51A0CF",
          "invert_powerline": true,
          "powerline_symbol": "",
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "lua"
        },
        {
          "background": "#111111",
          "foreground": "#925837",
          "invert_powerline": true,
          "powerline_symbol": "",
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "rust"
        },
        {
          "background": "#3B4252",
          "foreground": "#51A0CF",
          "invert_powerline": true,
          "powerline_symbol": "",
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "cmake"
        },
        {
          "background": "#4063D8",
          "foreground": "#055b9c",
          "style": "plain",
          "template": "  <#eceff4>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "dart"
        },
        {
          "background": "#4063D8",
          "foreground": "#ce092f",
          "style": "plain",
          "template": " ﮽ <#eceff4>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "angular"
        },
        {
          "background": "#81a1c1",
          "foreground": "#eceff4",
          "style": "plain",
          "template": "<#1e293b>(</>{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}<#1e293b>)</>",
          "type": "nx"
        },
        {
          "background": "#81a1c1",
          "foreground": "#316ce4",
          "style": "plain",
          "template": "<#eceff4>(</>{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<#ffffff>)</>",
          "type": "kubectl"
        },
        {
          "background": "#AE1401",
          "foreground": "#eceff4",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "ruby"
        },
        {
          "background": "#FEAC19",
          "foreground": "#eceff4",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "foreground": "#eceff4",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": "  {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "type": "aws"
        },
        {
          "background": "#A3BE8C",
          "foreground": "#011627",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "playing_icon": " ",
            "track_separator": " ~ "
          },
          "template": " {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Track }}{{ end }} ",
          "style": "powerline",
          "type": "spotify"
        },
        {
          "background": "#a3be8c",
          "background_templates": [
            "{{ if .Error }}p:c-battery-state-error{{ end }}",
            "{{ if lt .Percentage 15 }}p:c-battery-15-less{{ end }}",
            "{{ if and (gt .Percentage 16) (lt .Percentage 30) }}p:c-battery-30-less{{ end }}",
            "{{ if and (gt .Percentage 31) (lt .Percentage 45) }}p:c-battery-45-less{{ end }}",
            "{{ if and (gt .Percentage 46) (lt .Percentage 55) }}p:c-battery-55-less{{ end }}",
            "{{ if and (gt .Percentage 56) (lt .Percentage 70) }}p:c-battery-70-less{{ end }}",
            "{{ if and (gt .Percentage 71) (lt .Percentage 90) }}p:c-battery-90-less{{ end }}",
            "{{ if and (gt .Percentage 91) (lt .Percentage 100) }}p:c-battery-90-less{{ end }}"
          ],
          "foreground": "#3B4252",
          "invert_powerline": true,
          "powerline_symbol": "",
          "style": "powerline",
          "template": "{{ if eq \"True\" (title (default \"False\" .Env.DISABLE_SEGMENT_BATTERY)) }}{{ else }}{{ if not .Error }}{{ if eq \"Charging\" .State.String }} {{ else if eq \"Discharging\" .State.String }} {{ else if eq \"Full\" .State.String }}~ {{ else }}? {{ end }}{{ if lt .Percentage 15 }}{{ else if and (gt .Percentage 16) (lt .Percentage 30) }}{{ else if and (gt .Percentage 31) (lt .Percentage 45) }}{{ else if and (gt .Percentage 46) (lt .Percentage 55)}}{{ else if and (gt .Percentage 56) (lt .Percentage 70) }}{{ else if and (gt .Percentage 71) (lt .Percentage 90) }}{{ else }}{{ end }} {{ .Percentage }}%{{ else }}!{{ end }}{{ end }}",
          "type": "battery"
        },
        {
          "background": "#eceff4",
          "foreground": "#3B4252",
          "invert_powerline": true,
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }}  ",
          "trailing_diamond": "─╮",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#eceff4",
          "style": "plain",
          "template": "╰─",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "foreground": "#eceff4",
          "style": "plain",
          "template": "─╯",
          "type": "text"
        }
      ],
      "type": "rprompt"
    }
  ],
  "palette": {
    "c-badge-folder": "#FFD770",
    "c-badge-text": "#212121",
    "c-badge-white": "#FAFAFA",
    "c-badge-return-custom": "#E7B9FF",
    "c-badge-return-fail-term": "#FF8A80",
    "c-badge-return-success": "#B2FF59",
    "c-battery-15-less": "#FF8A80",
    "c-battery-30-less": "#FFD180",
    "c-battery-45-less": "#FFE57F",
    "c-battery-55-less": "#FFFF8D",
    "c-battery-70-less": "#F4FF81",
    "c-battery-90-less": "#B9F6CA",
    "c-battery-100-less": "#CCFF90",
    "c-battery-state-error": "#FF867F",
    "c-date-time-morning": "#FFFF8D",
    "c-date-time-noon": "#FFF64F",
    "c-date-time-afternoon": "#FFC400",
    "c-date-time-evening": "#C0CFFF",
    "c-date-time-night": "#83B9FF",
    "c-exec-fast": "#C6FF00",
    "c-exec-normal": "#FFFF00",
    "c-exec-slow": "#FFD180",
    "c-exec-slower": "#FF867F",
    "c-git-ahead": "#6EFFFF",
    "c-git-behind": "#FFA06D",
    "c-git-ahead-behind": "#C0CFFF",
    "c-git-normal": "#66FFA6",
    "c-git-staging": "#FFD740",
    "c-git-staging-working": "#FFB2FF",
    "c-git-upstream-gone": "#FF867F",
    "c-git-working": "#84FFFF",
    "c-project-node": "#9CFF57",
    "c-project-node-error": "#FF867F",
    "c-project-python": "#FFE873",
    "c-secondary-ellipsis": "#FFFF8D",
    "c-shell-state-ssh-active": "#BAFFFF",
    "c-shell-state-root-active": "#9FFFE0",
    "c-shell-state-root-ssh-active": "#FFB2FF",
    "c-wakatime-undertime": "#A7FFEB",
    "c-wakatime-warm-up": "#FFFFB3",
    "c-wakatime-working": "#FFD180",
    "c-wakatime-quota": "#FFD0B0",
    "c-wakatime-overtime": "#FF8A80"
  },
  "transient_prompt": {
    "foreground": "#B48EAD",
    "foreground_templates": [
      "{{ if gt .Code 0 }}#BF616A{{ end }}"
    ],
    "template": "❯ "
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 2
}
