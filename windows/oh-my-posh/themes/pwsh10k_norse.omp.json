{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#ffffff",
          "foreground": "#000000",
          "leading_diamond": "╭─",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#a3be8c",
          "foreground": "#000000",
          "powerline_symbol": "",
          "style": "powerline",
          "template": "  ",
          "type": "root"
        },
        {
          "background": "#81a1c1",
          "foreground": "#e4e4e4",
          "powerline_symbol": "",
          "properties": {
            "folder_separator_icon": "/",
            "home_icon": "~",
            "max_depth": 2,
            "style": "letter"
          },
          "style": "powerline",
          "template": "  {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#A3BE8C",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ebcb8b{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#A3BE8C{{ end }}"
          ],
          "foreground": "#000000",
          "powerline_symbol": "",
          "properties": {
            "branch_icon": " ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " ﯙ {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#3B4252",
          "background_templates": [
            "{{ if gt .Code 0 }}#bf616a{{ end }}"
          ],
          "foreground": "#ebcb8b",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ if gt .Code 0 }}{{ .Meaning }}{{ else }}✔{{ end }} ",
          "type": "exit"
        },
        {
          "background": "#ebcb8b",
          "foreground": "#000000",
          "invert_powerline": true,
          "powerline_symbol": "",
          "style": "powerline",
          "template": " {{ .FormattedMs }}  ",
          "type": "executiontime"
        },
        {
          "background": "#689f63",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#00acd7",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "#4063D8",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "julia"
        },
        {
          "background": "#81a1c1",
          "foreground": "#3b4252",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#4063D8",
          "foreground": "#ec2729",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "java"
        },
        {
          "background": "#4063D8",
          "foreground": "#0d6da8",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Unsupported }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "dotnet"
        },
        {
          "background": "#3B4252",
          "foreground": "#51A0CF",
          "invert_powerline": true,
          "powerline_symbol": "",
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "lua"
        },
        {
          "background": "#111111",
          "foreground": "#925837",
          "invert_powerline": true,
          "powerline_symbol": "",
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "rust"
        },
        {
          "background": "#3B4252",
          "foreground": "#51A0CF",
          "invert_powerline": true,
          "powerline_symbol": "",
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "cmake"
        },
        {
          "background": "#4063D8",
          "foreground": "#055b9c",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "dart"
        },
        {
          "background": "#4063D8",
          "foreground": "#ce092f",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "angular"
        },
        {
          "background": "#81a1c1",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#1e293b>(</>{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}<#1e293b>)</>",
          "type": "nx"
        },
        {
          "background": "#81a1c1",
          "foreground": "#316ce4",
          "style": "plain",
          "template": "<#ffffff>(</>{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<#ffffff>)</>",
          "type": "kubectl"
        },
        {
          "background": "#AE1401",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "ruby"
        },
        {
          "background": "#FEAC19",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": "  {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "type": "aws"
        },
        {
          "background": "#1DB954",
          "foreground": "#011627",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "playing_icon": " ",
            "track_separator": " ~ "
          },
          "template": " {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Track }}{{ end }} ",
          "style": "powerline",
          "type": "spotify"
        },
        {
          "background": "#a3be8c",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#A3BE8C{{end}}",
            "{{if eq \"Discharging\" .State.String}}#EBCB8B{{end}}",
            "{{if eq \"Full\" .State.String}}#8FBCBB{{end}}"
          ],
          "foreground": "#000000",
          "invert_powerline": true,
          "powerline_symbol": "",
          "style": "powerline",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}% ",
          "type": "battery"
        },
        {
          "background": "#ffffff",
          "foreground": "#000000",
          "invert_powerline": true,
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }}  ",
          "trailing_diamond": "─╮",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "╰─",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "─╯",
          "type": "text"
        }
      ],
      "type": "rprompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "{{ .Shell }}> "
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 2
}
